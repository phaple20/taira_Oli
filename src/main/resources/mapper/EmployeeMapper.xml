<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taira.box.mapper.EmployeeMapper">

    <select id="getMobileByEmployee" resultType="com.taira.box.bean.model.EmployeeBean">
        select * from employee where mobile = #{mobile}
    </select>

</mapper>